  shorttitle = {{{CHIMERE}} 2013},
  author = {Menut, L. and Bessagnet, B. and Khvorostyanov, D. and Beekmann, M. and Blond, N. and Colette, A. and Coll, I. and Curci, G. and Foret, G. and Hodzic, A. and Mailler, S. and Meleux, F. and Monge, J.-L. and Pison, I. and Siour, G. and Turquety, S. and Valari, M. and Vautard, R. and Vivanco, M. G.},
