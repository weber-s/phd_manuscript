@article{bourgeoisAtmospheric2018,
  title = {Atmospheric Nitrate Export in Streams along a Montane to Urban Gradient},
  author = {Bourgeois, Ilann and Savarino, Joel and Némery, Julien and Caillon, Nicolas and Albertin, Sarah and Delbart, Franck and Voisin, Didier and Clément, Jean-Christophe},
  date = {2018-08-15},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
  volume = {633},
  pages = {329--340},
  issn = {0048-9697},
  doi = {10.1016/j.scitotenv.2018.03.141},
  url = {http://www.sciencedirect.com/science/article/pii/S0048969718309008},
  urldate = {2020-08-06},
  abstract = {Nitrogen (N) emissions associated with urbanization exacerbate the atmospheric N influx to remote ecosystems – like mountains –, leading to well-documented detrimental effects on ecosystems (e.g., soil acidification, pollution of freshwaters). Here, the importance and fate of N deposition in a watershed was evaluated along a montane to urban gradient, using a multi-isotopic tracers approach (Δ17O, δ15N, δ18O of nitrate, δ2H and δ18O of water). In this setting, the montane streams had higher proportions of atmospheric nitrate compared to urban streams, and exported more atmospheric nitrate on a yearly basis (0.35 vs 0.10 kg-Nha−1yr−1). In urban areas, nitrate exports were driven by groundwater, whereas in the catchment head nitrate exports were dominated by surface runoff. The main sources of nitrate to the montane streams were microbial nitrification and atmospheric deposition, whereas microbial nitrification and sewage leakage contributed most to urban streams. Based on the measurement of δ15N and δ18O-NO3−, biological processes such as denitrification or N assimilation were not predominant in any streams in this study. The observed low δ15N and δ18O range of terrestrial nitrate (i.e., nitrate not coming from atmospheric deposition) in surface water compared to literature suggests that atmospheric deposition may be underestimated as a direct source of N.},
  file = {/home/samuel/Zotero/storage/SASQNEFZ/Bourgeois et al. - 2018 - Atmospheric nitrate export in streams along a mont.pdf;/home/samuel/Zotero/storage/NVJ2E9ZR/S0048969718309008.html},
  keywords = {Deposition,Isotope,Lautaret,Nitrogen,Subalpine,ΔO},
  langid = {english}
}

@article{bourgeoisFoliar2019,
  title = {Foliar Uptake of Atmospheric Nitrate by Two Dominant Subalpine Plants: Insights from in Situ Triple-Isotope Analysis},
  shorttitle = {Foliar Uptake of Atmospheric Nitrate by Two Dominant Subalpine Plants},
  author = {Bourgeois, Ilann and Clément, Jean-Christophe and Caillon, Nicolas and Savarino, Joël},
  date = {2019},
  journaltitle = {New Phytologist},
  volume = {223},
  pages = {1784--1794},
  issn = {1469-8137},
  doi = {10.1111/nph.15761},
  url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.15761},
  urldate = {2020-08-06},
  abstract = {The significance of foliar uptake of nitrogen (N) compounds in natural conditions is not well understood, despite growing evidence of its importance to plant nutrition. In subalpine meadows, N-limitation fosters the dominance of specific subalpine plant species, which in turn ensures the provision of essential ecosystems services. Understanding how these plants absorb N and from which sources is important in predicting ecological consequences of increasing N deposition. Here, we investigate the sources of N to plants from subalpine meadows with distinct land-use history in the French Alps, using the triple isotopes (Δ17O, δ18O, and δ15N) of plant tissue nitrate (NO3−). We use this approach to evaluate the significance of foliar uptake of atmospheric NO3− (NO3−atm). The foliar uptake of NO3−atm accounted for 4–16\% of the leaf NO3− content, and contributed more to the leaf NO3− pool after peak biomass. Additionally, the gradual 15N enrichment of NO3− from the soil to the leaves reflected the contribution of NO3−atm assimilation to plants’ metabolism. The present study confirms that foliar uptake is a potentially important pathway for NO3−atm into subalpine plants. This is of major significance as N emissions (and deposition) are predicted to increase globally in the future.},
  file = {/home/samuel/Zotero/storage/6XTUF9DJ/nph.html},
  keywords = {atmospheric nitrate,foliar uptake,nitrogen deposition,stable isotopes,subalpine plants},
  langid = {english},
  number = {4}
}

