@article{rattanavarahaReactive2011,
  ids = {rattanavarahaReactiveOxidantPotential2011},
  title = {The Reactive Oxidant Potential of Different Types of Aged Atmospheric Particles: {{An}} Outdoor Chamber Study},
  shorttitle = {The Reactive Oxidant Potential of Different Types of Aged Atmospheric Particles},
  author = {Rattanavaraha, Weruka and Rosen, Eli and Zhang, Haofei and Li, Qianfeng and Pantong, Karun and Kamens, Richard M.},
