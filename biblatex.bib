@article{morenoOxidative2017,
  title = {Oxidative Potential of Subway {{PM}} 2.5},
  author = {Moreno, Teresa and Kelly, Frank J. and Dunster, Chrissi and Oliete, Ana and Martins, Vânia and Reche, Cristina and Minguillón, Maria Cruz and Amato, Fulvio and Capdevila, Marta and de Miguel, Eladio and Querol, Xavier},
  date = {2017-01},
  journaltitle = {Atmospheric Environment},
  shortjournal = {Atmospheric Environment},
  volume = {148},
  pages = {230--238},
  issn = {13522310},
  doi = {10.1016/j.atmosenv.2016.10.045},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S1352231016308524},
  urldate = {2020-02-03},
  abstract = {Air quality in subway systems is of interest not only because particulate matter (PM) concentrations can be high, but also because of the peculiarly metalliferous chemical character of the particles, most of which differ radically from those of outdoor ambient air. We report on the oxidative potential (OP) of PM2.5 samples collected in the Barcelona subway system in different types of stations. The PM chemical composition of these samples showed typically high concentrations of Fe, Total Carbon, Ba, Cu, Mn, Zn and Cr sourced from rail tracks, wheels, catenaries, brake pads and pantographs. Two toxicological indicators of oxidative activity, ascorbic acid (AA) oxidation (expressed as OPAA mgÀ1 or OPAA mÀ3) and glutathione (GSH) oxidation (expressed as OPGSH mgÀ1 or OPGSH mÀ3), showed low OP for all samples (compared with outdoor air) but considerable variation between stations (0.9e2.4 OPAA mgÀ1; 0.4e1.9 OPGSH mgÀ1). Results indicate that subway PM toxicity is not related to variations in PM2.5 concentrations produced by ventilation changes, tunnel works, or station design, but may be affected more by the presence of metallic trace elements such as Cu and Sb sourced from brakes and pantographs. The OP assays employed do not reveal toxic effects from the highly ferruginous component present in subway dust.},
  file = {/home/samuel/Zotero/storage/ULDMD4ME/Moreno et al. - 2017 - Oxidative potential of subway PM 2.5.pdf},
  langid = {english},
  options = {useprefix=true}
}

