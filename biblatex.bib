@report{zhouDevelopment2017,
  title = {Development, Characterization and First Deployment of an Improved Onlinereactive Oxygen Species Analyzer},
  author = {Zhou, Jun and Bruns, Emily A. and Zotter, Peter and Stefenelli, Giulia and Prévôt, André S. H. and Baltensperger, Urs and EI-Haddad, Imad and Dommen, Josef},
  date = {2017-05-29},
  institution = {{Aerosols/Laboratory Measurement/Instruments and Platforms}},
  doi = {10.5194/amt-2017-161},
  url = {https://www.atmos-meas-tech-discuss.net/amt-2017-161/amt-2017-161.pdf},
  urldate = {2020-02-03},
  abstract = {\&lt;p\&gt;\&lt;strong\&gt;Abstract.\&lt;/strong\&gt; Inhalation of atmospheric particles is linked to human diseases. Reactive oxygen species (ROS) present in these atmospheric aerosols may play an important role. However, the ROS content in aerosols and their formation pathways are still largely unknown. Here, we have developed an online and offline ROS analyzer using a 2\&amp;\#8217;,7\&amp;\#8217;-dichlorofluorescin (DCFH) based assay. The sensitivity of the ROS analyzer was characterized using a suite of model organic compounds. The instrument detection limit determined as three times the noise is 1.3\&amp;\#8201;nmol\&amp;\#8201;L\&lt;sup\&gt;\&amp;\#8722;1\&lt;/sup\&gt; for offline analysis and 2\&amp;\#8201;nmol\&amp;\#8201;m\&lt;sup\&gt;\&amp;\#8722;3\&lt;/sup\&gt; of sampled air when the instrument is operated online at a fluorescence response time of approximately 8\&amp;\#8201;min, while the offline method detection limit is 9\&amp;\#8201;nmol\&amp;\#8201;L\&lt;sup\&gt;\&amp;\#8722;1\&lt;/sup\&gt; to 13\&amp;\#8201;nmol\&amp;\#8201;L\&lt;sup\&gt;\&amp;\#8722;1\&lt;/sup\&gt;. Potential interferences from gas phase O\&lt;sub\&gt;3\&lt;/sub\&gt; and NO\&lt;sub\&gt;x\&lt;/sub\&gt;, matrix effects of particulate SO\&lt;sub\&gt;4\&lt;/sub\&gt;\&lt;sup\&gt;2\&amp;\#8722;\&lt;/sup\&gt; and NO\&lt;sub\&gt;3\&lt;/sub\&gt;\&lt;sup\&gt;\&amp;\#8722;\&lt;/sup\&gt; were tested, but not observed. Fe\&lt;sup\&gt;3+\&lt;/sup\&gt; had no influence on the ROS signal while soluble Fe\&lt;sup\&gt;2+\&lt;/sup\&gt; reduced it if present at high concentrations in the extracts. Both online and offline methods were applied to identify the ROS content of different aerosol types, i.e., ambient aerosols as well as fresh and aged aerosols from wood combustion emissions. The stability of the ROS was assessed by comparing the ROS concentration measured by the same instrumentation online in-situ with offline measurements. We also analyzed the evolution of ROS in specific samples by conducting the analysis after storage times of up to four months. The ROS were observed to decay with increasing storage duration. From their decay behavior, ROS in secondary organic aerosol (SOA) can be separated into short- and long-lived fractions, with an average half-life of \textasciitilde\&amp;\#8201;1.7\&amp;\#8201;h and \textasciitilde\&amp;\#8201;432\&amp;\#8201;d, respectively. All these measurements showed consistently that, on average 60\&amp;\#8201;\&amp;\#177;\&amp;\#8201;20\&amp;\#8201;\% of the ROS were very reactive and disappeared during the filter storage time. This demonstrates the importance of a fast online measurement of ROS.\&lt;/p\&gt;},
  file = {/home/samuel/Zotero/storage/ZL3BNR5P/Zhou et al. - 2017 - Development, characterization and first deployment.pdf},
  langid = {english},
  type = {preprint}
}

@article{zhouDevelopment2018,
  ids = {zhouDevelopmentCharacterizationFirst2018},
  title = {Development, Characterization and First Deployment of an Improved Online Reactive Oxygen Species Analyzer},
  author = {Zhou, Jun and Bruns, Emily A. and Zotter, Peter and Stefenelli, Giulia and Prévôt, André S. H. and Baltensperger, Urs and El-Haddad, Imad and Dommen, Josef},
  date = {2018-01-09},
  journaltitle = {Atmospheric Measurement Techniques},
  shortjournal = {Atmos. Meas. Tech.},
  volume = {11},
  pages = {65--80},
  issn = {1867-8548},
  doi = {10.5194/amt-11-65-2018},
  url = {https://www.atmos-meas-tech.net/11/65/2018/},
  urldate = {2020-02-03},
  abstract = {Abstract. Inhalation of atmospheric particles is linked to human diseases. Reactive oxygen species (ROS) present in these atmospheric aerosols may play an important role. However, the ROS content in aerosols and their formation pathways are still largely unknown. Here, we have developed an online and offline ROS analyzer using a 2′,7′-dichlorofluorescin (DCFH) based assay. The ROS analyzer was calibrated with H2O2 and its sensitivity was characterized using a suite of model organic compounds. The instrument detection limit determined as 3~times the noise is 1.3 nmol L−1 for offline analysis and 2 nmol m−3 of sampled air when the instrument is operated online at a fluorescence response time of approximately 8 min, while the offline method detection limit is 18 nmol L−1. Potential interferences from gas-phase O3 and NO2 as well as matrix effects of particulate SO42− and NO3− were tested, but not observed. Fe3+ had no influence on the ROS signal, while soluble Fe2+ reduced it if present at high concentrations in the extracts. Both online and offline methods were applied to identify the ROS content of different aerosol types, i.e., ambient aerosols as well as fresh and aged aerosols from wood combustion emissions. The stability of the ROS was assessed by comparing the ROS concentration measured by the same instrumentation online in situ with offline measurements. We also analyzed the evolution of ROS in specific samples by conducting the analysis after storage times of up to 4~months. The ROS were observed to decay with increasing storage duration. From their decay behavior, ROS in secondary organic aerosol (SOA) can be separated into short- and long-lived fractions. The half-life of the short-lived fraction was 1.7 ± 0.4 h, while the half-life of the long-lived fraction could not be determined with our uncertainties. All these measurements showed consistently that on average 60 ± 20 \% of the ROS were very reactive and disappeared during the filter storage time. This demonstrates the importance of a fast online measurement of ROS.},
  file = {/home/samuel/Zotero/storage/2I7VWHXT/Zhou et al. - 2018 - Development, characterization and first deployment.pdf;/home/samuel/Zotero/storage/FJ78PCJJ/Zhou et al. - 2018 - Development, characterization and first deployment.pdf},
  langid = {english},
  number = {1}
}

@article{zhouParticlebound2018,
  title = {Particle-Bound Reactive Oxygen Species ({{PB}}-{{ROS}}) Emissions and Formation Pathways in Residential Wood Smoke under Different Combustion and Aging Conditions},
  author = {Zhou, Jun and Zotter, Peter and Bruns, Emily A. and Stefenelli, Giulia and Bhattu, Deepika and Brown, Samuel and Bertrand, Amelie and Marchand, Nicolas and Lamkaddam, Houssni and Slowik, Jay G. and Prévôt, André S. H. and Baltensperger, Urs and Nussbaumer, Thomas and El-Haddad, Imad and Dommen, Josef},
  date = {2018-05-18},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {18},
  pages = {6985--7000},
  issn = {1680-7316},
  doi = {10.5194/acp-18-6985-2018},
  url = {https://www.atmos-chem-phys.net/18/6985/2018/},
  urldate = {2019-12-06},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Wood combustion emissions can induce oxidative stress in the human respiratory tract by reactive oxygen species (ROS) in the aerosol particles, which are emitted either directly or formed through oxidation in the atmosphere. To improve our understanding of the particle-bound ROS (PB-ROS) generation potential of wood combustion emissions, a suite of smog chamber (SC) and potential aerosol mass (PAM) chamber experiments were conducted under well-determined conditions for different combustion devices and technologies, different fuel types, operation methods, combustion regimes, combustion phases, and aging conditions. The PB-ROS content and the chemical properties of the aerosols were quantified by a novel ROS analyzer using the DCFH (2\textsuperscript{′},7\textsuperscript{′}-dichlorofluorescin) assay and a high-resolution time-of-flight aerosol mass spectrometer (HR-ToF-AMS). For all eight combustion devices tested, primary PB-ROS concentrations substantially increased upon aging. The level of primary and aged PB-ROS emission factors (EF\textsubscript{ROS}) were dominated by the combustion device (within different combustion technologies) and to a greater extent by the combustion regimes: the variability within one device was much higher than the variability of EF\textsubscript{ROS} from different devices. Aged EF\textsubscript{ROS} under bad combustion conditions were ∼\&thinsp;2–80 times higher than under optimum combustion conditions. EF\textsubscript{ROS} from automatically operated combustion devices were on average 1 order of magnitude lower than those from manually operated devices, which indicates that automatic combustion devices operated at optimum conditions to achieve near-complete combustion should be employed to minimize PB-ROS emissions. The use of an electrostatic precipitator decreased the primary and aged ROS emissions by a factor of ∼\&thinsp;1.5 which is however still within the burn-to-burn variability. The parameters controlling the PB-ROS formation in secondary organic aerosol were investigated by employing a regression model, including the fractions of the mass-to-charge ratios \emph{m}∕\emph{z} 44 and 43 in secondary organic aerosol (SOA; \emph{f}\textsubscript{44−SOA} and \emph{f}\textsubscript{43−SOA}), the OH exposure, and the total organic aerosol mass. The regression model results of the SC and PAM chamber aging experiments indicate that the PB-ROS content in SOA seems to increase with the SOA oxidation state, which initially increases with OH exposure and decreases with the additional partitioning of semi-volatile components with lower PB-ROS content at higher OA concentrations, while further aging seems to result in a decay of PB-ROS. The results and the special data analysis methods deployed in this study could provide a model for PB-ROS analysis of further wood or other combustion studies investigating different combustion conditions and aging methods.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/2B56JP8I/Zhou et al_2018_Particle-bound reactive oxygen species (PB-ROS) emissions and formation.pdf;/home/samuel/Zotero/storage/ZHEF4UN9/2018.html},
  langid = {english},
  number = {10}
}

@article{zhouPredominance2019,
  title = {Predominance of Secondary Organic Aerosol to Particle-Bound Reactive Oxygen Species Activity in Fine Ambient Aerosol},
  author = {Zhou, Jun and Elser, Miriam and Huang, Ru-Jin and Krapf, Manuel and Fröhlich, Roman and Bhattu, Deepika and Stefenelli, Giulia and Zotter, Peter and Bruns, Emily A. and Pieber, Simone M. and Ni, Haiyan and Wang, Qiyuan and Wang, Yichen and Zhou, Yaqing and Chen, Chunying and Xiao, Mao and Slowik, Jay G. and Brown, Samuel and Cassagnes, Laure-Estelle and Daellenbach, Kaspar R. and Nussbaumer, Thomas and Geiser, Marianne and Prévôt, André S. H. and El-Haddad, Imad and Cao, Junji and Baltensperger, Urs and Dommen, Josef},
  date = {2019-12-05},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {19},
  pages = {14703--14720},
  issn = {1680-7316},
  doi = {10.5194/acp-19-14703-2019},
  url = {https://www.atmos-chem-phys.net/19/14703/2019/},
  urldate = {2019-12-06},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Reactive oxygen species (ROS) are believed to contribute to the adverse health effects of aerosols. This may happen by inhaled particle-bound (exogenic) ROS (PB-ROS) or by ROS formed within the respiratory tract by certain aerosol components (endogenic ROS). We investigated the chemical composition of aerosols and their exogenic ROS content at the two contrasting locations Beijing (China) and Bern (Switzerland). We apportioned the ambient organic aerosol to different sources and attributed the observed water-soluble PB-ROS to them. The oxygenated organic aerosol (OOA, a proxy for secondary organic aerosol, SOA) explained the highest fraction of the exogenic ROS concentration variance at both locations. We also characterized primary and secondary aerosol emissions generated from different biogenic and anthropogenic sources in smog chamber experiments. The exogenic PB-ROS content in the OOA from these emission sources was comparable to that in the ambient measurements. Our results imply that SOA from gaseous precursors of different anthropogenic emission sources is a crucial source of water-soluble PB-ROS and should be additionally considered in toxicological and epidemiological studies in an adequate way besides primary emissions. The importance of PB-ROS may be connected to the seasonal trends in health effects of PM reported by epidemiological studies, with elevated incidences of adverse effects in warmer seasons, which are accompanied by more-intense atmospheric oxidation processes.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/VUPSN9ZU/Zhou et al_2019_Predominance of secondary organic aerosol to particle-bound reactive oxygen.pdf;/home/samuel/Zotero/storage/I9ATXHBG/2019.html},
  langid = {english},
  number = {23}
}

