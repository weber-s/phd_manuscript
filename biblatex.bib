@article{mullerFormation2012,
  title = {Formation of 3-Methyl-1,2,3-Butanetricarboxylic Acid via Gas Phase Oxidation of Pinonic Acid – a Mass Spectrometric Study of {{SOA}} Aging},
  author = {Müller, L. and Reinnig, M.-C. and Naumann, K. H. and Saathoff, H. and Mentel, T. F. and Donahue, N. M. and Hoffmann, T.},
