@article{menutImpact2020,
  title = {Impact of Lockdown Measures to Combat {{Covid}}-19 on Air Quality over Western {{Europe}}},
  author = {Menut, Laurent and Bessagnet, Bertrand and Siour, Guillaume and Mailler, Sylvain and Pennel, Romain and Cholakian, Arineh},
