@article{seoEffects2018,
  title = {Effects of Meteorology and Emissions on Urban Air Quality: A Quantitative Statistical Approach to Long-Term Records (1999\&ndash;2016) in {{Seoul}}, {{South Korea}}},
  shorttitle = {Effects of Meteorology and Emissions on Urban Air Quality},
  author = {Seo, Jihoon and Park, Doo-Sun R. and Kim, Jin Young and Youn, Daeok and Lim, Yong Bin and Kim, Yumi},
  date = {2018-07-25},
  journaltitle = {Atmospheric Chemistry and Physics Discussions},
  pages = {1--30},
  issn = {1680-7316},
  doi = {10.5194/acp-2018-621},
  url = {https://www.atmos-chem-phys-discuss.net/acp-2018-621/},
  urldate = {2018-07-26},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Together with emissions of air pollutants and precursors, meteorological conditions play important roles in local air quality through accumulation or ventilation, regional transport, and atmospheric chemistry. In this study, we extensively investigated multi-timescale meteorological effects on the urban air pollution using the long-term measurements data of PM\textsubscript{10}, SO\textsubscript{2}, NO\textsubscript{2}, CO, and O\textsubscript{3} and meteorological variables over the period of 1999\&ndash;2016 in Seoul, South Korea. The long-term air quality data were decomposed into trend-free short-term components and long-term trends by the Kolmogorov-Zurbenko filter, and the effects of meteorology and emissions were quantitatively isolated using a multiple linear regression with meteorological variables. In terms of short-term variability, intercorrelations among the pollutants and meteorological variables and composite analysis of synoptic meteorological fields exhibited that the warm and stagnant conditions in the migratory high-pressure system are related to the high PM\textsubscript{10} and primary pollutant while the strong irradiance and low NO\textsubscript{2} by high winds at the rear of cyclone are related to the high O\textsubscript{3}. In terms of long-term trends, decrease in PM\textsubscript{10} (\&minus;1.75µgm\textsuperscript{\&minus;3}yr\textsuperscript{\&minus;1}) and increase in O\textsubscript{3} (+0.88ppbyr\textsuperscript{\&minus;1}) in Seoul were largely contributed by the meteorology-related trends (\&minus;0.94µgm\textsuperscript{\&minus;3}yr\textsuperscript{\&minus;1} for PM\textsubscript{10} and +0.47ppbyr\textsuperscript{\&minus;1} for O\textsubscript{3}), which were attributable to the subregional scale wind speeds increase. Comparisons with estimated local emissions and socioeconomic indices like GDP growth and fuel consumptions indicate probable influences of the 2008 global economic recession as well as the enforced regulations from the mid-2000s on the emission-related trends of PM\textsubscript{10} and other primary pollutants. Change rates of local emissions and transport term of long-term components calculated by the tracer continuity equation revealed a decrease in contributions of local emissions to the primary pollutants including PM\textsubscript{10} and an increase in contributions of local secondary productions to O\textsubscript{3}. This study shows meteorological conditions related to the episodic air pollution events in short-term and provides insights into the current environmental policies and regulations by isolation of emission-related trends in long-term.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/XVZTFKDC/Seo et al. - 2018 - Effects of meteorology and emissions on urban air .pdf;/home/samuel/Zotero/storage/JPXNF9MM/acp-2018-621.html},
  langid = {english}
}

