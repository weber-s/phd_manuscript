@article{canonacoSoFi2013,
  title = {{{SoFi}}, an {{IGOR}}-Based Interface for the Efficient Use of the Generalized Multilinear Engine ({{ME}}-2) for the Source Apportionment: {{ME}}-2 Application to Aerosol Mass Spectrometer Data},
  shorttitle = {{{SoFi}}, an {{IGOR}}-Based Interface for the Efficient Use of the Generalized Multilinear Engine ({{ME}}-2) for the Source Apportionment},
  author = {Canonaco, F. and Crippa, M. and Slowik, J. G. and Baltensperger, U. and Prévôt, A. S. H.},
