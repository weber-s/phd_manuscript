@article{miyazakiSeasonal2012,
  title = {Seasonal Variations of Stable Carbon Isotopic Composition and Biogenic Tracer Compounds of Water-Soluble Organic Aerosols in a Deciduous Forest},
  author = {Miyazaki, Y. and Fu, P. Q. and Kawamura, K. and Mizoguchi, Y. and Yamanoi, K.},
  date = {2012-02-03},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {12},
  pages = {1367--1376},
  publisher = {{Copernicus GmbH}},
  issn = {1680-7316},
  doi = {10.5194/acp-12-1367-2012},
  url = {https://www.atmos-chem-phys.net/12/1367/2012/},
  urldate = {2020-05-26},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} To investigate the seasonal changes in biogenic water-soluble organic carbon (WSOC) aerosols in a boreal forest, aerosol samples were collected continuously in the canopy of a deciduous forest in northern Japan during 2009–2010. Stable carbon isotopic composition of WSOC (\&delta;\textsuperscript{13}C\textsubscript{WSOC}) in total suspended particulate matter (TSP) exhibited a distinct seasonal cycle, with lower values from June through September (−25.5\&plusmn;0.5 \&permil;). This cycle follows the net CO\textsubscript{2} exchange between the forest ecosystem and the atmosphere, indicating that \&delta;\textsuperscript{13}C\textsubscript{WSOC} likely reflects the biological activity at the forest site. WSOC concentrations showed the highest values in early summer and autumn. Positive matrix factorization (PMF) analysis indicated that the factor in which biogenic secondary organic aerosols (BSOAs) dominated accounted for \textasciitilde 40 \% of the highest concentrations of WSOC, where BSOAs mostly consisted of α-/β-pinene SOA. In addition, primary biological aerosol particles (PBAPs) made similar contributions (\textasciitilde 57 \%) to the WSOC near the forest floor in early summer. This finding indicates that the production of both primary and secondary WSOC aerosols is important during the growing season in a deciduous forest. The methanesulfonic acid (MSA) maximum was also found in early summer and had a distinct vertical gradient with larger concentrations near the forest floor. Together with the similar vertical gradients found for WSOC and \&delta;\textsuperscript{13}C\textsubscript{WSOC} as well as the α-/β-pinene SOA tracers, our results indicate that the forest floor, including ground vegetation and soil, acts as a significant source of WSOC in TSP within a forest canopy at the study site.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/AXYDXG5P/Miyazaki et al. - 2012 - Seasonal variations of stable carbon isotopic comp.pdf;/home/samuel/Zotero/storage/5SXUZ5BY/2012.html},
  langid = {english},
  number = {3}
}

@article{monteiroStrengths2018,
  title = {Strengths and Weaknesses of the {{FAIRMODE}} Benchmarking Methodology for the Evaluation of Air Quality Models},
  author = {Monteiro, A. and Durka, P. and Flandorfer, C. and Georgieva, E. and Guerreiro, C. and Kushta, J. and Malherbe, L. and Maiheu, B. and Miranda, A. I. and Santos, G. and Stocker, J. and Trimpeneers, E. and Tognet, F. and Stortini, M. and Wesseling, J. and Janssen, S. and Thunis, P.},
  date = {2018-05},
  journaltitle = {Air Quality, Atmosphere \& Health},
  volume = {11},
