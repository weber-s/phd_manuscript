@article{fangRelative2019,
  title = {Relative Humidity and {{O}}{\textsubscript{3}} Concentration as Two Prerequisites for Sulfate Formation},
  author = {Fang, Yanhua and Ye, Chunxiang and Wang, Junxia and Wu, Yusheng and Hu, Min and Lin, Weili and Xu, Fanfan and Zhu, Tong},
  date = {2019-10-02},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {19},
  pages = {12295--12307},
  publisher = {{Copernicus GmbH}},
  issn = {1680-7316},
  doi = {10.5194/acp-19-12295-2019},
  url = {https://www.atmos-chem-phys.net/19/12295/2019/},
  urldate = {2020-05-13},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Sulfate formation mechanisms have been discussed extensively but are still disputed. In this work, a year-long particulate matter (PM\textsubscript{2.5}) sampling campaign was conducted together with measurements of gaseous pollutant concentrations and meteorological parameters in Beijing, China, from March 2012 to February 2013. The sulfur oxidation ratio (SOR), an indicator of secondary sulfate formation, displayed a clear summer peak and winter valley, even though no obvious seasonal variations in sulfate mass concentration were observed. A rapid rise in the SOR was found at a relative humidity (RH) threshold of ∼45\&thinsp;\% or an O\textsubscript{3} concentration threshold of ∼35\&thinsp;ppb, allowing us to first introduce the idea that RH and O\textsubscript{3} concentrations are two prerequisites for rapid sulfate formation via multiphase reactions. In the case of the RH threshold, this is consistent with current understanding of the multiphase formation of sulfate, since it relates to the semisolid-to-liquid phase transition of atmospheric aerosols. Correlation analysis between SOR and aerosol water content (AWC) further backed this up. In the case of the O\textsubscript{3} concentration threshold, this is consistent with the consumption of liquid oxidants in multiphase sulfate formation. The thresholds introduced here lead us to a better understanding of the sulfate formation mechanisms and sulfate formation variations. H\textsubscript{2}O\textsubscript{2} might be the major oxidant of sulfate formation, since another liquid-phase oxidant, O\textsubscript{3}, has previously been shown to be unimportant. The seasonal variations in sulfate formation could be accounted for by variations in the RH and O\textsubscript{3} prerequisites. For example, over the year-long study, the fastest SO\textsubscript{2}-to-sulfate conversion occurred in summer, which was associated with the highest values of O\textsubscript{3} (and also H\textsubscript{2}O\textsubscript{2}) concentration and RH. The SOR also displayed variations with pollution levels; i.e. the SOR increased with PM\textsubscript{2.5} in all seasons. Such variations were primarily associated with a transition from the slow gas-phase formation of sulfate to rapid multiphase reactions, since RH increased higher than its prerequisite value of around 45\&thinsp;\% as pollution evolved. In addition, the self-catalytic nature of sulfate formation (i.e. the formation of hydrophilic sulfate aerosols under high RH conditions results in an increase in aerosol water content, which results in greater particle volume for further multiphase sulfate formation) also contributed to variations among the pollution scenarios.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/GI2FNMU3/Fang et al. - 2019 - Relative humidity and Osub3sub concentration .pdf;/home/samuel/Zotero/storage/XUG9VAWW/acp-19-12295-2019.html},
  langid = {english},
  number = {19}
}

