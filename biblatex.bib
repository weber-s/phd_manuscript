@article{saContributions2019,
  title = {Contributions of Biomass-Burning, Urban, and Biogenic Emissions to the Concentrations and Light-Absorbing Properties of Particulate Matter in Central {{Amazonia}} during the Dry Season},
  author = {de Sá, Suzane S. and Rizzo, Luciana V. and Palm, Brett B. and Campuzano-Jost, Pedro and Day, Douglas A. and Yee, Lindsay D. and Wernis, Rebecca and Isaacman-VanWertz, Gabriel and Brito, Joel and Carbone, Samara and Liu, Yingjun J. and Sedlacek, Arthur and Springston, Stephen and Goldstein, Allen H. and Barbosa, Henrique M. J. and Alexander, M. Lizabeth and Artaxo, Paulo and Jimenez, Jose L. and Martin, Scot T.},
  date = {2019-06-18},
  journaltitle = {Atmospheric Chemistry and Physics},
  volume = {19},
  pages = {7973--8001},
  issn = {1680-7316},
  doi = {10.5194/acp-19-7973-2019},
  url = {https://www.atmos-chem-phys.net/19/7973/2019/acp-19-7973-2019.html},
  urldate = {2019-07-04},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Urbanization and deforestation have important impacts on atmospheric particulate matter (PM) over Amazonia. This study presents observations and analysis of PM\textsubscript{1} concentration, composition, and optical properties in central Amazonia during the dry season, focusing on the anthropogenic impacts. The primary study site was located 70\&thinsp;km downwind of Manaus, a city of over 2 million people in Brazil, as part of the GoAmazon2014/5 experiment. A high-resolution time-of-flight aerosol mass spectrometer (AMS) provided data on PM\textsubscript{1} composition, and aethalometer measurements were used to derive the absorption coefficient \emph{b}\textsubscript{abs,BrC} of brown carbon (BrC) at 370\&thinsp;nm. Non-refractory PM\textsubscript{1} mass concentrations averaged 12.2\&thinsp;µg\&thinsp;m\textsuperscript{−3} at the primary study site, dominated by organics (83\&thinsp;\%), followed by sulfate (11\&thinsp;\%). A decrease in \emph{b}\textsubscript{abs,BrC} was observed as the mass concentration of nitrogen-containing organic compounds decreased and the organic PM\textsubscript{1} O:C ratio increased, suggesting atmospheric bleaching of the BrC components. The organic PM\textsubscript{1} was separated into six different classes by positive-matrix factorization (PMF), and the mass absorption efficiency \emph{E}\textsubscript{abs} associated with each factor was estimated through multivariate linear regression of \emph{b}\textsubscript{abs,BrC} on the factor loadings. The largest \emph{E}\textsubscript{abs} values were associated with urban (2.04±0.14\&thinsp;m\textsuperscript{2}\&thinsp;g\textsuperscript{−1}) and biomass-burning (0.82±0.04 to 1.50±0.07\&thinsp;m\textsuperscript{2}\&thinsp;g\textsuperscript{−1}) sources. Together, these sources contributed at least 80\&thinsp;\% of \emph{b}\textsubscript{abs,BrC} while accounting for 30\&thinsp;\% to 40\&thinsp;\% of the organic PM\textsubscript{1} mass concentration. In addition, a comparison of organic PM\textsubscript{1} composition between wet and dry seasons revealed that only part of the 9-fold increase in mass concentration between the seasons can be attributed to biomass burning. Biomass-burning factor loadings increased by 30-fold, elevating its relative contribution to organic PM\textsubscript{1} from about 10\&thinsp;\% in the wet season to 30\&thinsp;\% in the dry season. However, most of the PM\textsubscript{1} mass (\&gt;60\&thinsp;\%) in both seasons was accounted for by biogenic secondary organic sources, which in turn showed an 8-fold seasonal increase in factor loadings. A combination of decreased wet deposition and increased emissions and oxidant concentrations, as well as a positive feedback on larger mass concentrations are thought to play a role in the observed increases. Furthermore, fuzzy \emph{c}-means clustering identified three clusters, namely “baseline”, “event”, and “urban” to represent different pollution influences during the dry season. The baseline cluster, representing the dry season background, was associated with a mean mass concentration of 9±3\&thinsp;µg\&thinsp;m\textsuperscript{−3}. This concentration increased on average by 3\&thinsp;µg\&thinsp;m\textsuperscript{−3} for both the urban and the event clusters. The event cluster, representing an increased influence of biomass burning and long-range transport of African volcanic emissions, was characterized by remarkably high sulfate concentrations. The urban cluster, representing the influence of Manaus emissions on top of the baseline, was characterized by an organic PM\textsubscript{1} composition that differed from the other two clusters. The differences discussed suggest a shift in oxidation pathways as well as an accelerated oxidation cycle due to urban emissions, in agreement with findings for the wet season.{$<$}/p{$>$}},
  file = {/home/samuel/Zotero/storage/4U4KN34P/Sá et al. - 2019 - Contributions of biomass-burning, urban, and bioge.pdf;/home/samuel/Zotero/storage/2IFXAJWR/acp-19-7973-2019.html},
  langid = {english},
  number = {12}
}

@article{saffariGlobal2014,
  title = {Global {{Perspective}} on the {{Oxidative Potential}} of {{Airborne Particulate Matter}}: {{A Synthesis}} of {{Research Findings}}},
  shorttitle = {Global {{Perspective}} on the {{Oxidative Potential}} of {{Airborne Particulate Matter}}},
  author = {Saffari, Arian and Daher, Nancy and Shafer, Martin M. and Schauer, James J. and Sioutas, Constantinos},
  date = {2014-07},
  journaltitle = {Environmental Science \& Technology},
  shortjournal = {Environ. Sci. Technol.},
  volume = {48},
  pages = {7576--7583},
  issn = {0013-936X, 1520-5851},
  doi = {10.1021/es500937x},
  url = {https://pubs.acs.org/doi/10.1021/es500937x},
  urldate = {2020-02-03},
  abstract = {An emerging hypothesis in the field of air pollution is that oxidative stress is one of the important pathways leading to adverse health effects of airborne particulate matter (PM). To advance our understanding of sources and chemical elements contributing to aerosol oxidative potential and provide global comparative data, we report here on the biological oxidative potential associated with size-segregated airborne PM in different urban areas of the world, measured by a biological (cell-based) reactive oxygen species (ROS) assay. Our synthesis indicates a generally greater intrinsic PM oxidative potential as well as higher levels of exposure to redox-active PM in developing areas of the world. Moreover, on the basis of our observations, smaller size fractions are generally associated with higher intrinsic ROS activity compared with larger PM size fractions. Another important outcome of our study is the identification of major species and sources that are associated with ROS activity. Water-soluble transition metals (e.g., Fe, Ni, Cu, Cr, Mn, Zn and V) and water-soluble organic carbon (WSOC) showed consistent correlations with the oxidative potential of airborne PM across different urban areas and size ranges. The major PM sources associated with these chemical species include residual/fuel oil combustion, traffic emissions, and secondary organic aerosol formation, indicating that these sources are major drivers of PM-induced oxidative potential. Moreover, comparison of ROS activity levels across different seasons indicated that photochemical aging increases the intrinsic oxidative potential of airborne PM.},
  file = {/home/samuel/Zotero/storage/T8GA6GAB/Saffari et al. - 2014 - Global Perspective on the Oxidative Potential of A.pdf},
  langid = {english},
  number = {13}
}

