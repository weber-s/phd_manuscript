@article{nawrotOxidative2009,
  title = {Oxidative Properties of Ambient {{PM2}}.5 and Elemental Composition: {{Heterogeneous}} Associations in 19 {{European}} Cities},
  shorttitle = {Oxidative Properties of Ambient {{PM2}}.5 and Elemental Composition},
  author = {Nawrot, Tim S. and Kuenzli, Nino and Sunyer, Jordi and Shi, Tingming and Moreno, Teresa and Viana, Mar and Heinrich, Joachim and Forsberg, Bertil and Kelly, Frank J. and Sughis, Muhammad and Nemery, Benoit and Borm, Paul},
  date = {2009-09-01},
