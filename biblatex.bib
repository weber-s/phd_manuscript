  author = {Kunugi, Yushi and Arimura, Toshi H. and Iwata, Kazuyuki and Komatsu, Eiji and Hirayama, Yoshie},
  date = {2018-11-26},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
